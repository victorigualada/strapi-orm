  "author": "victorigualada <victorigualada@gmail.com> (https://github.com/victorigualada)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorigualada/strapi-orm/issues"
  },
  "homepage": "https://github.com/victorigualada/strapi-orm#readme",
  "dependencies": {
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/qs": "^6.9.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sql": "^2.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
